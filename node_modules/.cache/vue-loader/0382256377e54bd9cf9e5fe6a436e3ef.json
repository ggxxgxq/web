<DropdownItem>\n                  <sui-icon name=\"pencil alternate\"/>\n                  ÂèëË°®ÊñáÁ´†\n                </DropdownItem>\n              </router-link>\n              <DropdownItem>\n                <sui-icon name=\"star\"/>\n                Êî∂ËóèÂàóË°®\n              </DropdownItem>\n              <DropdownItem>\n                <sui-icon name=\"key\"/>\n                Ë¥¶Âè∑ÁÆ°ÁêÜ\n              </DropdownItem>\n              <DropdownItem divided @click.native=\"logout\">\n                <sui-icon name=\"power off\"/>\n                ÈÄÄÂá∫ÁôªÂΩï\n              </DropdownItem>\n            </DropdownMenu>\n          </Dropdown>\n          <sui-image class=\"avatar-img\" v-else circular @click.native=\"logInOnModel.show = true\"\n                     :src=\"require('../../assets/img/avatar/anonymous-avatar.jpg')\"/>\n        </div>\n        <!-- Â§¥ÂÉè END -->\n        <!-- Ê∂àÊÅØÈÄöÁü• -->\n        <div class=\"notification\" v-click-outside=\"() => { this.showNotificationCard = false }\">\n          <Badge dot>\n            <Icon type=\"ios-notifications-outline\" size=\"26\" @click=\"showNotificationCard = true\"/>\n          </Badge>\n          <div class=\"notification-card-wrapper\" v-show=\"showNotificationCard\">\n            <sui-card class=\"notification-card\">\n              <sui-button-group attached=\"top\" basic>\n                <sui-button>\n                  <Badge dot :offset=\"[0,-5]\">Á≥ªÁªüÈÄöÁü•</Badge>\n                </sui-button>\n                <sui-button @click=\"toMessage\">\n                  <Badge dot :offset=\"[0,-5]\">Ê∂àÊÅØ‰∏≠ÂøÉ</Badge>\n                </sui-button>\n              </sui-button-group>\n              <sui-card-content>\n                <sui-list class=\"notification-list\" divided>\n                  <sui-list-item v-for=\"i in 30\">\n                    <router-link to=\"\"><b>Ëµ´Êú¨</b></router-link>\n                    &nbsp;ËØÑËÆ∫‰∫Ü&nbsp;\n                    <router-link to=\"\"><b>MarkdownËØ≠Ê≥ï</b></router-link>\n                    &nbsp;<Time time=\"2024-05-31 12:10\"/>\n                  </sui-list-item>\n                </sui-list>\n              </sui-card-content>\n            </sui-card>\n          </div>\n        </div>\n        <!-- Ê∂àÊÅØÈÄöÁü• END -->\n        <!-- ÊêúÁ¥¢Ê°Ü -->\n        <div class=\"ui search search-container\" @keydown.enter=\"toSearch\">\n          <div class=\"ui icon input\">\n            <input class=\"prompt\" v-model=\"q\" type=\"text\" placeholder=\"ÊêúÁ¥¢Ê†°Âõ≠ËµÑËÆØ\">\n            <i class=\"search icon\"></i>\n          </div>\n          <div class=\"results\">\n            ËøôÈáå‰ºöÊòæÁ§∫Êé®ËçêÁªìÊûúÔºåÊöÇÊó∂Êú™ÂÅö\n          </div>\n        </div>\n        <!-- ÊêúÁ¥¢Ê°Ü END -->\n      </div>\n      <!-- ÁôªÂΩïÊ≥®ÂÜåÊ®°ÊÄÅÊ°Ü -->\n      <Modal class=\"login-on-modal\" v-model=\"logInOnModel.show\" :title=\"logInOnModel.title\" :mask-closable=\"false\"\n             width=\"320\" scrollable footer-hide :styles=\"{top: '20%'}\">\n        <!-- ÁôªÂΩïË°®Âçï -->\n        <sui-form v-if=\"logInOnModel.isLogin\">\n          <sui-form-field>\n            <sui-input placeholder=\"ËØ∑ËæìÂÖ•Áî®Êà∑ÂêçÊàñÈÇÆÁÆ±\" v-model=\"loginForm.username\"/>\n          </sui-form-field>\n          <sui-form-field>\n            <sui-input type=\"password\" placeholder=\"ËØ∑ËæìÂÖ•ÂØÜÁ†Å\" v-model=\"loginForm.password\"/>\n          </sui-form-field>\n          <sui-form-field>\n            <sui-button fluid basic primary @click.prevent=\"login\">ÁôªÂΩï</sui-button>\n          </sui-form-field>\n          <sui-form-field>\n            <div style=\"color: #5c5c5c\">Ê≤°ÊúâË¥¶Âè∑Ôºü<a @click=\"showRegisterForm\">Ê≥®ÂÜå</a>\n              <a style=\"float:right\" href=\"#\">ÂøòËÆ∞ÂØÜÁ†Å</a>\n            </div>\n          </sui-form-field>\n          <sui-form-field>\n            <div style=\"color: #5c5c5c\">Á¨¨‰∏âÊñπË¥¶Âè∑ÁôªÂΩïÔºö</div>\n            <div class=\"oauth\">\n              <div class=\"oauth-bg\">\n                <img alt=\"ÂæÆÂçö\" src=\"../../assets/img/icon/weibo.svg\" @click=\"oauth\"></div>\n              <div class=\"oauth-bg\">\n                <img alt=\"ÂæÆ‰ø°\" src=\"../../assets/img/icon/wechat.svg\" @click=\"oauth\"></div>\n              <div class=\"oauth-bg\">\n                <img alt=\"GitHub\" src=\"../../assets/img/icon/github.svg\" @click=\"oauth\"></div>\n            </div>\n          </sui-form-field>\n        </sui-form>\n        <!-- Ê≥®ÂÜåË°®Âçï -->\n        <sui-form v-else>\n          <sui-form-field>\n            <sui-input placeholder=\"ËØ∑ËæìÂÖ•Áî®Êà∑Âêç\" v-model=\"logonForm.username\"/>\n          </sui-form-field>\n          <sui-form-field>\n            <sui-input type=\"email\" placeholder=\"ËØ∑ËæìÂÖ•ÈÇÆÁÆ±\" v-model=\"logonForm.email\"/>\n          </sui-form-field>\n          <sui-form-field>\n            <sui-input type=\"password\" placeholder=\"ËØ∑ËæìÂÖ•ÂØÜÁ†Å\" v-model=\"logonForm.password\"/>\n          </sui-form-field>\n          <sui-form-field>\n            <sui-input type=\"password\" placeholder=\"ËØ∑Á°ÆËÆ§ÂØÜÁ†Å\" v-model=\"logonForm.rePassword\"/>\n          </sui-form-field>\n          <sui-form-field>\n            <sui-button style=\"width: 100%\" basic primary @click.prevent=\"logon\">Ê≥®ÂÜå</sui-button>\n          </sui-form-field>\n          <sui-form-field>\n            <div style=\"overflow: hidden\">\n              <a style=\"float:right\" @click=\"showLoginForm\">Â∑≤ÊúâË¥¶Âè∑ÁôªÂΩï</a>\n            </div>\n          </sui-form-field>\n        </sui-form>\n        <!-- Ê≥®ÂÜåË°®ÂçïÁªìÊùü -->\n      </Modal>\n      <!-- ÁôªÂΩïÊ≥®ÂÜåÊ®°ÊÄÅÊ°Ü END -->\n    </header>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Header',\n  data () {\n    return {\n      q: '',\n      logInOnModel: {\n        show: false,\n        title: 'ÁôªÂΩï',\n        isLogin: true\n      },\n      loginForm: {\n        username: '',\n        password: ''\n      },\n      logonForm: {\n        username: '',\n        email: '',\n        password: '',\n        rePassword: ''\n      },\n      rules: {},\n      showNotificationCard: false\n    };\n  },\n  computed: {\n    user () {\n      return this.$store.state.user;\n    }\n  },\n  methods: {\n    showLoginForm () {\n      this.logInOnModel.title = 'ÁôªÂΩï';\n      this.logInOnModel.isLogin = true;\n    },\n    showRegisterForm () {\n      this.logInOnModel.title = 'Ê≥®ÂÜå';\n      this.logInOnModel.isLogin = false;\n    },\n    showErrorNotice (title, desc) {\n      this.$Notice.error({ title, desc });\n    },\n    // ÁôªÂΩï\n    login () {\n      if (this.loginForm.username.length === 0) {\n        this.showErrorNotice('ÁôªÂΩïÂ§±Ë¥•', 'ËØ∑Â°´ÂÜôÁî®Êà∑ÂêçÊàñÈÇÆÁÆ±');\n      } else if (this.loginForm.password.length === 0) {\n        this.showErrorNotice('ÁôªÂΩïÂ§±Ë¥•', 'ËØ∑Â°´ÂÜôÂØÜÁ†Å');\n      } else {\n        // TODO ÂêéÊúüÈúÄË¶ÅÊê¨Âà∞ vuex\n        this.$axios.post('/api/user/login', this.loginForm)\n          .then(res => {\n            let result = res.data;\n            if (result.success) {\n              this.logInOnModel.show = false;\n              // ‰øùÂ≠òÂà∞ vuex\n              this.$store.commit('userUpdate', result.data);\n              this.loginForm = {\n                username: '',\n                password: ''\n              };\n            }\n          });\n      }\n    },\n    // Ê≥®ÈîÄ\n    logout () {\n      this.$axios.post('/api/user/logout')\n        .then(res => {\n          this.$store.commit('userUpdate', null);\n        });\n    },\n    // Ê≥®ÂÜå\n    logon () {\n      if (this.logonForm.username.length === 0) {\n        this.showErrorNotice('Ê≥®ÂÜåÂ§±Ë¥•', 'ËØ∑Â°´ÂÜôÁî®Êà∑Âêç');\n      } else if (this.logonForm.email.length === 0 ||\n        !/^[a-zA-Z0-9_-]+@[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)+$/.test(this.logonForm.email)) {\n        this.showErrorNotice('Ê≥®ÂÜåÂ§±Ë¥•', 'ËØ∑Â°´ÂÜôÂêàÊ≥ïÁöÑÈÇÆÁÆ±');\n      } else if (this.logonForm.password.length === 0 ||\n        this.logonForm.password !== this.logonForm.rePassword) {\n        this.showErrorNotice('Ê≥®ÂÜåÂ§±Ë¥•', '‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥');\n      } else {\n        // TODO 2024/6/6 ÂêéÊúüÈúÄË¶ÅÊê¨Âà∞ vuex\n        this.$axios.post('/api/user/logon', this.logonForm)\n          .then(res => {\n            this.$Notice.success({ title: 'Bingo', desc: 'Ê≥®ÂÜåÊàêÂäü' });\n            this.showLoginForm();\n          });\n      }\n    },\n    // Á¨¨‰∏âÊñπÁôªÂΩï\n    oauth () {\n      this.$Notice.warning({ title: 'Hey', desc: 'ÂäüËÉΩÊ≠£Âú®ÂºÄÂèë‰∏≠ üöÄ' });\n    },\n    // ÂÖ®ÊñáÊêúÁ¥¢ÁïåÈù¢\n    toSearch () {\n      this.$router.push({ name: 'search', query: { q: this.q } });\n    },\n    // Ê∂àÊÅØ‰∏≠ÂøÉ\n    toMessage () {\n      this.showNotificationCard = false;\n      if (!this.$store.state.user) {\n        this.logInOnModel.show = true;\n      } else {\n        this.$router.push('/message');\n      }\n    }\n  },\n  mounted () {\n    // TODO 2024/6/6 ‰πüÂèØ‰ª•Âú®ËøôÈáåÂä†ËΩΩ WebSocket\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n\n.header-wrapper {\n  height: 70px;\n}\n\nheader {\n  position: fixed;\n  z-index: 2;\n  width: 100%;\n  height: 70px;\n  background: #fff;\n  box-shadow: 0 0 8px 0 rgba(0, 0, 0, .1);\n}\n\n.container {\n  width: 1200px;\n  margin: 0 auto;\n  overflow: hidden;\n\n  .logo {\n    float: left;\n    height: 55px;\n    margin-top: 10px;\n  }\n\n  nav {\n    float: left;\n    margin-left: 30px;\n    line-height: 65px;\n\n    ul {\n      list-style-type: none;\n      padding-top: 5px;\n      font-size: 16px;\n\n      li {\n        display: inline-block;\n        margin-right: 20px;\n\n        a {\n          color: #aaa;\n        }\n\n        a:hover {\n          color: #ff8364;\n        }\n      }\n    }\n  }\n\n  .avatar-wrapper {\n    float: right;\n    width: 58px;\n    height: 58px;\n    margin-top: 7px;\n\n    .avatar-img {\n      width: 100%;\n      height: 100%;\n      margin-right: 0;\n    }\n  }\n\n  .notification {\n    float: right;\n    margin-top: 22px;\n    margin-right: 30px;\n\n    .notification-card-wrapper {\n      position: absolute;\n      margin-top: 20px;\n      margin-left: -300px;\n\n      .notification-card {\n        width: 400px;\n\n        .notification-list {\n          max-height: 400px;\n          overflow: auto;\n          margin-right: -7px;\n          padding-right: 5px;\n          font-size: .85em;\n\n          .item {\n            padding-top: 10px;\n            margin-bottom: 10px;\n\n            a {\n              color: #455A64;\n            }\n\n            a:hover {\n              color: #FF8364;\n            }\n\n            .ivu-time {\n              color: #7a7a7a;\n            }\n          }\n        }\n      }\n    }\n  }\n\n  .search-container {\n    float: right;\n    margin-top: 18px;\n    margin-right: 25px;\n  }\n\n  .ivu-form-item:last-child {\n    margin-bottom: 0;\n  }\n}\n\n.login-on-modal {\n  .oauth {\n    display: flex;\n    align-items: center;\n    justify-content: space-around;\n    margin-top: 15px;\n\n    .oauth-bg {\n      width: 45px;\n      height: 45px;\n      border-radius: 50%;\n      background-color: #f4f8fb;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n\n      img {\n        height: 1.7rem;\n        vertical-align: bottom;\n        cursor: pointer;\n      }\n    }\n  }\n}\n</style>\n"]}]}